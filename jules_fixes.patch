+from playwright.sync_api import sync_playwright, Page, expect
+import sys
+
+def verify_page(page: Page):
+    # Listen for all console events and print them to stderr
+    page.on("console", lambda msg: print(f"BROWSER CONSOLE: {msg.type.upper()} {msg.text}", file=sys.stderr))
+
+    # Navigate to the local development server
+    page.goto("http://localhost:5173/")
+
+    # Wait for the main heading to be visible to ensure the page has loaded
+    try:
+        heading = page.get_by_role("heading", name="Real-Time Filler Word Detection")
+        expect(heading).to_be_visible(timeout=10000)
+        print("SUCCESS: Heading found!")
+        page.screenshot(path="jules-scratch/verification/landing_page_success.png")
+    except Exception as e:
+        print(f"ERROR: Could not find heading. The page is likely blank.", file=sys.stderr)
+        page.screenshot(path="jules-scratch/verification/landing_page_blank.png")
+
+
+def main():
+    with sync_playwright() as p:
+        browser = p.chromium.launch(headless=True)
+        page = browser.new_page()
+        verify_page(page)
+        browser.close()
+
+if __name__ == "__main__":
+    main()
